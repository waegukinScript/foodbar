    // ================Contact Success and Error message Area =================
    #success.modal.modal-message.fade(role='dialog')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              i.fa.fa-close
            h2 Thank you
            p Your message is successfully sent...
    // Modals error
    #error.modal.modal-message.fade(role='dialog')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              i.fa.fa-close
            h2 Sorry !
            p  Something went wrong 
