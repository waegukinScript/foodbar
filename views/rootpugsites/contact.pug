    // ================Header Menu Area =================
    header.header_area
      .main_menu
        nav.navbar.navbar-expand-lg.navbar-light
          .container
            // Brand and toggle get grouped for better mobile display
            a.navbar-brand.logo_h(href='index.html')
              img.logo-1(src='img/logo.png', alt='')
              img.logo-2(src='img/logo-2.png', alt='')
            button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            // Collect the nav links, forms, and other content for toggling
            #navbarSupportedContent.collapse.navbar-collapse.offset
              ul.nav.navbar-nav.menu_nav.ml-auto
                li.nav-item
                  a.nav-link(href='index.html') Home
                li.nav-item
                  a.nav-link(href='about-us.html') About
                li.nav-item
                  a.nav-link(href='menu.html') Menu
                li.nav-item
                  a.nav-link(href='book-table.html') book a table
                li.nav-item.submenu.dropdown
                  a.nav-link.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') Pages
                  ul.dropdown-menu
                    li.nav-item
                      a.nav-link(href='gallery.html') Gallery
                    li.nav-item
                      a.nav-link(href='elements.html') Elements
                li.nav-item.submenu.dropdown
                  a.nav-link.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') Blog
                  ul.dropdown-menu
                    li.nav-item
                      a.nav-link(href='blog.html') Blog
                    li.nav-item
                      a.nav-link(href='single-blog.html') Blog Details
                li.nav-item.active
                  a.nav-link(href='contact.html') Contact
    // ================Header Menu Area =================
    // ================ Banner Area =================
    section.banner_area
      .banner_inner.d-flex.align-items-center
        .container
          .banner_content.text-center
            h1 Contact Us
            .page_link
              a(href='index.html') Home
              a(href='contact.html') Contact Us
        .shape.shape1
        .shape.shape2
        .shape.shape3
        .shape.shape4
        .shape.shape5
        .shape.shape6
        .shape.shape7
    // ================End Banner Area =================
    // ================Contact Area =================
    section.contact_area.p_120
      .container
        #mapBox.mapBox(data-lat='40.701083', data-lon='-74.1522848', data-zoom='13', data-info='PO Box CT16122 Collins Street West, Victoria 8007, Australia.', data-mlat='40.701083', data-mlon='-74.1522848')
        .row
          .col-lg-3
            .contact_info
              .info_item
                i.lnr.lnr-home
                h6 California, United States
                p Santa monica bullevard
              .info_item
                i.lnr.lnr-phone-handset
                h6
                  a(href='#') 00 (440) 9865 562
                p Mon to Fri 9am to 6 pm
              .info_item
                i.lnr.lnr-envelope
                h6
                  a(href='#') support@colorlib.com
                p Send us your query anytime!
          .col-lg-9
            form#contactForm.row.contact_form(action='contact_process.php', method='post', novalidate='novalidate')
              .col-md-6
                .form-group
                  input#name.form-control(type='text', name='name', placeholder='Enter your name')
                .form-group
                  input#email.form-control(type='email', name='email', placeholder='Enter email address')
                .form-group
                  input#subject.form-control(type='text', name='subject', placeholder='Enter Subject')
              .col-md-6
                .form-group
                  textarea#message.form-control(name='message', rows='1', placeholder='Enter Message')
              .col-md-12.text-right
                button.btn.submit_btn(type='submit', value='submit') Send Message
    // ================Contact Area =================
    // ================ start footer Area  =================
    footer.footer-area.p_120
      .container
        .row
          .col-lg-2.col-md-6.col-sm-6
            .single-footer-widget.tp_widgets
              h6.footer_title Top Products
              ul.list
                li
                  a(href='#') Managed Website
